{
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-debugger": "warn",
    "no-duplicate-case": "error",
    "no-empty": "warn",
    "no-extra-semi": "warn",
    "no-func-assign": "error",
    "no-irregular-whitespace": "warn",
    "no-unreachable": "error",
    "curly": "warn",
    "dot-notation": "warn",
    "eqeqeq": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  }
}
